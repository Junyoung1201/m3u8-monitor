{
    "manifest_version": 3,
    "name": "m3u8-monitor",
    "incognito": "split",
    "version": "1.0",
    "permissions": ["webRequest", "tabs", "storage","activeTab","scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
  }
  